// Place your settings in this file to overwrite default and user settings.
{
  "search.exclude": {
    "**/node_modules": true,
    "**/bower_components": true,
    "**/dist": true,
    "**/coverge": true
  },
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "node_modules": true
  },
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.codeLens": false,
  "cSpell.words": [
    "deeplink",
    "ulid",
    "upsert",
    "usecase"
  ]
}
